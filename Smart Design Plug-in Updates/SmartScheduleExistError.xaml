<Window x:Class="Smart_Design_Plug_in_Updates.SmartScheduleExistError"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Smart_Design_Plug_in_Updates"
             mc:Ignorable="d" 
             d:DesignHeight="160" d:DesignWidth="350">
    <Grid>
        <StackPanel>
            <TextBox x:Name="ErrorName" Margin="0 30 0 0" HorizontalAlignment="Center" FontSize="14" FontWeight="Regular" Foreground="RoyalBlue" BorderBrush="Transparent">Smart Schedule Already Exist</TextBox>
            <StackPanel Margin="0 24 0 0" Background="#eee" >
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width=".7*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="Close" Height="25" Margin="5 12 5 18" Grid.Column="2" Click="Close_Click" >Close</Button>
                    <Button x:Name="Register" Height="25" Margin="5 12 5 18" Grid.Column="1" Click="Register_Click" >Show Existing Data</Button>
                </Grid>

            </StackPanel>

        </StackPanel>
    </Grid>
</Window>
